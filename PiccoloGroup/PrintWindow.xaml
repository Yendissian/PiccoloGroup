<Window x:Class="PiccoloGroup.PrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PrintWindow" Height="600" Width="500"
        MinHeight="600" MinWidth="500">
    <Grid Background="White">
        <DocumentViewer Name="PrintView" HorizontalAlignment="Left" Margin="0,0,0,51"/>
        <Button Name="CloseButton" Content="Close" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10,0,0,10" Height="30" Width="472" Click="CloseButton_Click"/>
    </Grid>
</Window>

